<jaxws:bindings
        jxb:extensionBindingPrefixes="jaxws jxb xjc"
        version="2.0"
        xmlns="http://java.sun.com/xml/ns/jaxb"
        xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
        xmlns:xs="http://www.w3.org/2001/XMLSchema"
        xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
        xmlns:htd="http://www.example.org/WS-HT"
        xmlns:htda="http://www.example.org/WS-HT/api"
        xmlns:htdt="http://www.example.org/WS-HT/api/xsd"
        xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
        xmlns:jaxws="http://java.sun.com/xml/ns/jaxws">        

    <jaxws:bindings schemaLocation="../../../target/wsdl/ws-humantask.wsdl">
    
        <jaxws:bindings node="wsdl:definitions"> 
            <jaxws:package name="org.apache.hise.engine.wsdl"/>
        </jaxws:bindings>
       
    </jaxws:bindings>

</jaxws:bindings>
